algoritmo "semnome"

var

DINHEIRO, V : real

inicio
      V <- 16000
      Escreva("quanto de dinheiro você tem? R$ ")
      Leia(DINHEIRO)

      Se (DINHEIRO >= V) Entao
         escreva("Você tem dinheiro suficiente para ir a disney")
      SeNao
         escreva("Você não tem dinheiro ainda :(")
      FimSe
fimalgoritmo
